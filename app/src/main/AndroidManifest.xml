<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DogPong.pong">

    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dogpong_launcher"
        android:label="Dog Pong"
        android:roundIcon="@mipmap/dogpong_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.DogPong.pong.MyApp">
        <activity
            android:name="com.DogPong.pong.SplashActivity" android:theme="@style/SplashTheme"
            android:parentActivityName="com.DogPong.pong.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.DogPong.pong.MainActivity"
            android:parentActivityName="com.DogPong.pong.MainActivity"
            android:label="Retour"/>
        <activity android:name="com.DogPong.pong.TeamActivity"
            android:parentActivityName="com.DogPong.pong.MainActivity"
            android:label="Retour"/>
    </application>

</manifest>